<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 모서리 둥글게 만들기 -->
    <corners android:radius="16dp" />

    <!-- 테두리 색상과 두께 설정 -->
    <stroke
        android:width="0dp"
        android:color="#FF6200EE" />

    <!-- 배경 색상 설정 (선택 사항) -->
    <solid android:color="#F6F6F6" />
</shape>